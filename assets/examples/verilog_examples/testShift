#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55a1be17ddc0 .scope module, "test" "test" 2 4;
 .timescale -9 -12;
v0x55a1be17f350_0 .var/s "x", 3 0;
v0x55a1be17f7b0_0 .var/s "y", 3 0;
v0x55a1be17fba0_0 .var/s "z", 3 0;
    .scope S_0x55a1be17ddc0;
T_0 ;
    %delay 1000, 0;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x55a1be17f350_0, 0, 4;
    %delay 1000, 0;
    %load/vec4 v0x55a1be17f350_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
    %store/vec4 v0x55a1be17f7b0_0, 0, 4;
    %delay 1000, 0;
    %load/vec4 v0x55a1be17f7b0_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftr 4;
    %store/vec4 v0x55a1be17fba0_0, 0, 4;
    %end;
    .thread T_0;
    .scope S_0x55a1be17ddc0;
T_1 ;
    %vpi_call 2 17 "$monitor", "x = %d, y = %d, z = %d", v0x55a1be17f350_0, v0x55a1be17f7b0_0, v0x55a1be17fba0_0 {0 0 0};
    %delay 10000, 0;
    %vpi_call 2 18 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "testNum.v";
