#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5583f6d236e0 .scope module, "tb" "tb" 2 4;
 .timescale -9 -12;
v0x5583f6d35ee0_0 .var "clk1", 0 0;
v0x5583f6d35fb0_0 .net "cnt1", 9 0, v0x5583f6ceaba0_0;  1 drivers
v0x5583f6d36080_0 .var "rst1", 0 0;
S_0x5583f6d23870 .scope module, "inst1" "cnt" 2 9, 3 4 0, S_0x5583f6d236e0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 10 "cnt";
v0x5583f6cea7b0_0 .net "clk", 0 0, v0x5583f6d35ee0_0;  1 drivers
v0x5583f6ceaba0_0 .var "cnt", 9 0;
v0x5583f6d35ce0_0 .var "cnt_m", 9 0;
v0x5583f6d35da0_0 .net "rst", 0 0, v0x5583f6d36080_0;  1 drivers
E_0x5583f6ce9890 .event posedge, v0x5583f6cea7b0_0;
    .scope S_0x5583f6d23870;
T_0 ;
    %wait E_0x5583f6ce9890;
    %load/vec4 v0x5583f6d35da0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 10;
    %assign/vec4 v0x5583f6ceaba0_0, 0;
    %load/vec4 v0x5583f6ceaba0_0;
    %assign/vec4 v0x5583f6d35ce0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5583f6ceaba0_0;
    %addi 1, 0, 10;
    %assign/vec4 v0x5583f6ceaba0_0, 0;
    %load/vec4 v0x5583f6ceaba0_0;
    %cmpi/u 1023, 0, 10;
    %flag_inv 5; GE is !LT
    %jmp/0xz  T_0.2, 5;
    %load/vec4 v0x5583f6ceaba0_0;
    %assign/vec4 v0x5583f6ceaba0_0, 0;
T_0.2 ;
    %load/vec4 v0x5583f6d35ce0_0;
    %addi 2, 0, 10;
    %assign/vec4 v0x5583f6d35ce0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5583f6d236e0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5583f6d35ee0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x5583f6d236e0;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x5583f6d35ee0_0;
    %inv;
    %store/vec4 v0x5583f6d35ee0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5583f6d236e0;
T_3 ;
    %delay 1000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5583f6d36080_0, 0, 1;
    %delay 30000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5583f6d36080_0, 0, 1;
    %delay 200000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5583f6d36080_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5583f6d36080_0, 0, 1;
    %delay 15000000, 0;
    %vpi_call 2 23 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5583f6d236e0;
T_4 ;
    %vpi_call 2 27 "$dumpfile", "cnt_bn.vcd" {0 0 0};
    %vpi_call 2 28 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5583f6d236e0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
